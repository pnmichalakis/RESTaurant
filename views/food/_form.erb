<%= form_open(@food, method: 'patch') %>
 <%= input_tag(type: 'text', resource: 'food', name: 'name', value: food.name)%>
 <%= input_tag(type: 'text', resource: 'food', name: 'type', value: food.type)%>
 <%= input_tag(type: 'text', resource: 'food', name: 'cents', value: food.price)%>
 <%= input_tag(type: 'submit') %>
 <% form_close %>